{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "verbatimModuleSyntax": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "lib": ["ESNext"],
    "outDir": "./dist",
    "types": ["node", "hono"],
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "tsc-alias": {
    "resolveFullPaths": true
  }
}
