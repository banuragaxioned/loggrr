{
  "name": "loggrr",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "clean": "turbo clean && rm -rf node_modules && rm -rf .turbo",
    "db:studio": "turbo db:studio",
    "dev": "turbo dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "postinstall": "manypkg check",
    "lint": "turbo lint",
    "manypkg:fix": "manypkg fix",
    "pkg:update": "pnpm up --recursive -i --latest"
  },
  "prettier": "@axioned/prettier-config",
  "devDependencies": {
    "@axioned/prettier-config": "^1.0.5",
    "@manypkg/cli": "^0.23.0",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "prettier": "^3.5.3",
    "turbo": "^2.5.0",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@10.7.1",
  "engines": {
    "node": "22.x",
    "pnpm": ">=9.15.4"
  }
}
